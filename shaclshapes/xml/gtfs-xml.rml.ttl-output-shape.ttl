@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://ns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix gtfs: <http://vocab.gtfs.org/terms#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd1: <http://www.w3.org/2001/> .

<http://mapping.example.com/map_feed_0/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://transport.linkeddata.es/madrid/metro/feed/.*" ;
    sh:property [ sh:datatype xsd1:date ;
            sh:nodeKind sh:Literal ;
            sh:path schema:endDate ],
        [ sh:nodeKind sh:Literal ;
            sh:path schema:version ],
        [ sh:nodeKind sh:Literal ;
            sh:path dct:publisher ],
        [ sh:nodeKind sh:Literal ;
            sh:path dct:language ],
        [ sh:datatype xsd1:date ;
            sh:nodeKind sh:Literal ;
            sh:path schema:startDate ],
        [ sh:nodeKind sh:IRI ;
            sh:path foaf:page ] ;
    sh:targetClass gtfs:Feed .

